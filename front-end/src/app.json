{
    "version": "2.0.7",
    "externalLibs": [
        "https://apis.google.com/js/platform.js"
    ],
    "preloadFiles": [
        "environment-config",
        "functions.js",
        "app-proxy.js"
    ],
    "extraFiles": [
        "all-pages.js"
    ],
    "angularFiles": [
        "scripts.js",
        "runtime.js",
        "polyfills.js",
        "main.js"
    ],
    "cssFiles": [
        "styles.css"
    ],
    "match": [
        {
            "regex": ".*/sip/.*",
            "load": false
        },
        {
            "regex": ".*login\\.php.*",
            "load": false
        },
        {
            "regex": ".*/sei/publicacoes/.*",
            "load": false
        },
        {
            "regex": ".*controlador\\.php\\?acao\\=arvore_visualizar.*",
            "load": false
        },
        {
            "regex": ".*controlador\\.php\\?acao\\=grupo_email_selecionar.*",
            "load": false
        },
        {
            "regex": ".*controlador\\.php\\?acao\\=documento_enviar_email.*",
            "load": false
        },
        {
            "regex": ".*controlador\\.php\\?acao\\=protocolo_pesquisar.*",
            "load": false
        },
        {
            "regex": ".*controlador\\.php\\?acao\\=documento_assinar.*",
            "load": false
        },
        {
            "regex": ".*/sei/#/",
            "toolbar": false,
            "logged": true,
            "prepend": "#divInfraAreaTelaD",
            "files": [
                "pages/procedimento_controlar.js"
            ]
        },
        {
            "regex": ".*controlador\\.php\\?acao\\=procedimento_controlar.*",
            "toolbar": false,
            "logged": true,
            "prepend": "#divInfraAreaTelaD",
            "files": [
                "pages/procedimento_controlar.js"
            ]
        },
        {
            "regex": ".*controlador\\.php\\?acao\\=procedimento_trabalhar.*",
            "route": "/listeners/procedimento-trabalhar",
            "toolbar": true,
            "logged": true,
            "prepend": "#divInfraAreaTelaD",
            "files": [
                "pages/procedimento_trabalhar.js"
            ]
        },
        {
            "regex": ".*controlador\\.php\\?acao\\=editor_montar.*",
            "route": "/listeners/editor-montar",
            "toolbar": true,
            "logged": true,
            "before": "#divCarregando",
            "files": [
                "pages/editor_montar.js"
            ]
        },
        {
            "regex": ".*controlador\\.php\\?acao\\=procedimento_escolher_tipo.*",
            "route": "/listeners/procedimento-escolher-tipo",
            "toolbar": true,
            "logged": true,
            "prepend": "#divInfraAreaTelaD",
            "files": [
                "pages/procedimento_escolher_tipo.js"
            ]
        },
        {
            "regex": ".*",
            "route": "/listeners/petrvs",
            "toolbar": true,
            "logged": false,
            "prepend": "#divInfraAreaTelaD",
            "files": []
        }
    ]
}
